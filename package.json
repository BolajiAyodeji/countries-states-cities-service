    "trailingComma": "es5"
  },
  "name": "countries-states-cities-service",
  "author": "Alessandro Casazza",
  "module": "lib/index.js",
  "size-limit": [
    {
      "path": "lib",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@size-limit/preset-small-lib": "^4.5.5",
    "@types/jest": "^27.0.2",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "size-limit": "^5.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}
