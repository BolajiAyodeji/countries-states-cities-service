{
  "version": "0.1.0",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "package.json"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc --build",
    "test": "jest",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build"
    }
  },
  "prettier": {
